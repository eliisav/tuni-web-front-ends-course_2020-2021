{
  "name": "bloglist-tests",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "review-1": "cross-env REACT_APP_SOLUTION=solution-to-review-1 react-scripts start",
    "review-2": "cross-env REACT_APP_SOLUTION=solution-to-review-2 react-scripts start"
    ,
    "build": "react-scripts build"
    ,
    "test": "react-scripts test --watchAll=false src/your-solution",
    "test-review-1": "cross-env SOLUTION=solution-to-review-1 react-scripts test --watchAll=false src/reviews/solution-to-review-1",
    "test-review-2": "cross-env SOLUTION=solution-to-review-2 react-scripts test --watchAll=false src/reviews/solution-to-review-2"
    ,
    "eject": "react-scripts eject"
    ,
    "lint": "eslint ./src/your-solution",
    "lint-review-1": "eslint ./src/reviews/solution-to-review-1",
    "lint-review-2": "eslint ./src/reviews/solution-to-review-2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "cross-env": "^7.0.3"
  },
  "proxy": "http://localhost:3003"
}
